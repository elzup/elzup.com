{
	"parser": "babel-eslint",
	"extends": "eslint:recommended",
	"env": {
		"browser": true,
		"es6": true,
		"node": true,
		"jasmine": true
	},
	"plugins": [
		"react",
		"flowtype"
	],
	"rules": {
		"react/jsx-uses-vars": 1,
		"react/jsx-no-undef": 1,
		"indent": [
			2,
			"tab"
		],
		"semi": [
			2,
			"never"
		]
	}
}
