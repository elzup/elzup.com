{
	"name": "elzup.com",
	"version": "1.0.1",
	"description": "elzup.com Web Site",
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --env=jsdom",
		"eject": "react-scripts eject",
		"precommit": "lint-staged",
		"deploy":
			"npm run build && rsync -rtvpl ./build/ sakura:~/docker/repos/elzup.com-v5/public/"
	},
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/elzup/elzup.com"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/elzup/elzup.com/issues"
	},
	"homepage": "https://elzup.com",
	"dependencies": {
		"axios": "^0.19.0",
		"lodash": "^4.17.4",
		"normalize.css": "^8.0.0",
		"react": "^16.1.1",
		"react-dom": "^16.1.1",
		"react-fontawesome": "^1.1.0",
		"react-lazyload": "^2.3.0",
		"react-router": "^5.0.0",
		"react-router-dom": "^5.0.0",
		"react-scripts": "^1.0.14",
		"react-tooltip": "^3.4.0",
		"styled-components": "^2.2.1"
	},
	"devDependencies": {
		"@babel/cli": "7.8.4",
		"@babel/preset-env": "7.8.4",
		"babel-plugin-lodash": "3.3.4",
		"flow-bin": "0.117.0",
		"font-awesome": "4.7.0",
		"husky": "3.1.0",
		"lint-staged": "10.0.7",
		"node-noop": "1.0.0",
		"prettier": "1.19.1"
	},
	"lint-staged": {
		"*.js": [
			"prettier --use-tabs --no-semi --single-quote --trailing-comma es5 --write",
			"git add"
		]
	}
}
