{
	"name": "elzup.com",
	"version": "1.0.1",
	"description": "elzup.com Web Site",
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "qawolf test --headless",
		"eject": "react-scripts eject",
		"precommit": "lint-staged",
		"deploy": "npm run build && rsync -rtvpl ./build/ sakura:~/docker/repos/elzup.com-v5/public/"
	},
	"main": "index.js",
	"repository": {
		"type": "git",
		"url": "https://github.com/elzup/elzup.com"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/elzup/elzup.com/issues"
	},
	"homepage": "https://elzup.com",
	"dependencies": {
		"axios": "^0.21.0",
		"lodash": "^4.17.19",
		"normalize.css": "^8.0.0",
		"react": "^17.0.0",
		"react-dom": "^17.0.0",
		"react-fontawesome": "^1.1.0",
		"react-lazyload": "^3.0.0",
		"react-router": "^5.2.0",
		"react-router-dom": "^5.2.0",
		"react-scripts": "^4.0.0",
		"react-tooltip": "^4.2.7",
		"styled-components": "^5.1.1"
	},
	"devDependencies": {
		"@babel/cli": "7.12.1",
		"@babel/preset-env": "7.12.1",
		"babel-plugin-lodash": "3.3.4",
		"flow-bin": "0.137.0",
		"font-awesome": "4.7.0",
		"husky": "4.3.0",
		"jest-cli": "26.6.1",
		"jest-styled-components": "7.0.3",
		"lint-staged": "10.5.1",
		"node-noop": "1.0.0",
		"playwright": "1.5.2",
		"prettier": "2.1.2",
		"qawolf": "1.4.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.js": [
			"prettier --use-tabs --no-semi --single-quote --trailing-comma es5 --write",
			"git add"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
